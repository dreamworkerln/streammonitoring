package ru.kvanttelecom.tv.streammonitoring.core.dto.stream;


import lombok.Getter;
import ru.dreamworkerln.spring.utils.common.annotations.Default;

import java.io.Serializable;
import java.util.Objects;


public class StreamKey implements Serializable {

//    @Getter
//    private String serverName;
//    @Getter
//    private String streamName;

    @Getter
    private long serverId;
    @Getter
    private long streamId;

    public  static StreamKey from(long serverId, long streamId) {
        return new StreamKey(serverId, streamId);
    }


    @Default
    public StreamKey(long serverId, long streamId) {
        this.serverId = serverId;
        this.streamId = streamId;
    }

    @Override
    public boolean equals(Object o) {
        if (this == o) return true;
        if (!(o instanceof StreamKey)) return false;
        StreamKey streamKey = (StreamKey) o;
        return serverId == streamKey.serverId &&
            streamId == streamKey.streamId;
    }

    @Override
    public int hashCode() {
        return Objects.hash(serverId, streamId);
    }

    @Override
    public String toString() {
        return "StreamKey{" +
            "serverId=" + serverId +
            ", streamId=" + streamId +
            '}';
    }
}
